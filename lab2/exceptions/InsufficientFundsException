class InsufficientFundsException(OceanariumBaseException):
    """Исключение при недостатке средств на счету."""
    def __init__(self, required_amount, available_amount):
        self.required_amount = required_amount
        self.available_amount = available_amount
        super().__init__(f"Недостаточно средств: требуется {required_amount}, доступно {available_amount}")
